{
  "name": "x-by-y",
  "version": "2.0.1",
  "license": "MIT",
  "description": "Find X by its unique property Y",
  "author": {
    "name": "Jeff Held",
    "email": "jheld135@gmail.com",
    "url": "https://github.com/solkaz"
  },
  "homepage": "https://github.com/solkaz/x-by-y#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/solkaz/x-by-y.git"
  },
  "main": "dist/x-by-y.umd.js",
  "module": "dist/x-by-y.es5.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "typescript",
    "map",
    "es6-map"
  ],
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "husky": "^0.14.3",
    "jest": "^23.0.1",
    "lint-staged": "^7.1.2",
    "prettier": "^1.12.1",
    "rollup": "^0.59.3",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.14.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.8.3"
  },
  "scripts": {
    "start": "rollup -c ./rollup.config.js -w",
    "test": "jest --watch",
    "build": "rollup -c ./rollup.config.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "{src,__tests__}/**/*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ],
    "./**/*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
  }
}
